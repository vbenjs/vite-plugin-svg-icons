{
  "name": "vite-plugin-svg-icons",
  "version": "1.0.4",
  "description": "Vite Plugin for fast creating SVG sprites.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Vben",
  "files": [
    "dist",
    "register.d.ts",
    "client.d.ts",
    "module.d.ts"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "pinst --disable && npm run build",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "svg",
    "sprite",
    "svgo",
    "vben"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anncwb/vite-plugin-svg-icons"
  },
  "bugs": {
    "url": "https://github.com/anncwb/vite-plugin-svg-i cons/issues"
  },
  "homepage": "https://github.com/anncwb/vite-plugin-svg-icons/tree/master/#readme",
  "dependencies": {
    "@types/svgo": "^2.4.2",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "etag": "^1.8.1",
    "fs-extra": "^10.0.0",
    "svg-baker": "1.7.0",
    "svgo": "^2.7.0"
  },
  "peerDependencies": {
    "vite": ">=2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/etag": "^1.8.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.2",
    "husky": "^7.0.2",
    "jest": "^27.2.2",
    "lint-staged": "^11.1.2",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "tsup": "^5.2.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.10"
  }
}
